{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.death": {
		"initial_state":"default",
			"states": {
				"default": {
					"transitions": [
						{
							"dead": "!q.is_alive"
						}
					]
				},
				"dead": {
					"on_entry": ["/say I am dead!"],
					"transitions": [
						{
							"default": "q.is_alive"
						}
					]
				}
			}
		}
	}
}
